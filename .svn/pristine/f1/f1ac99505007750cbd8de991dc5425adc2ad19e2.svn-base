    <script type="text/javascript" language="javascript">
        function RemoveBill1(which) {

            document.getElementById('browseBill' + which).style.display = '';
            document.getElementById('viewBill' + which).style.display = 'none';
            document.getElementById("<%= hdnNewBill1.ClientID %>").value = '';
        }

        function RemoveBill2(which) {

            document.getElementById('browseBill' + which).style.display = '';
            document.getElementById('viewBill' + which).style.display = 'none';
            document.getElementById("<%= hdnNewBill2.ClientID %>").value = '';
        }

        function RemoveCertificate1() {

            document.getElementById('browseCert1').style.display = '';
            document.getElementById('viewCert1').style.display = 'none';
            document.getElementById("<%= hdnNewCerti1.ClientID %>").value = '';
        }

        function RemoveCertificate2() {

            document.getElementById('browseCert2').style.display = '';
            document.getElementById('viewCert2').style.display = 'none';
            document.getElementById("<%= hdnNewCerti2.ClientID %>").value = '';
        }


        function StartupScriptBills() {

            var ViewHTML = "window.open('_REPLACEURL?modalwin=1', 'View', 'left=100,top=30,screenX=100,screenY=30, height=550,width=840,toolbar=no,directories=no,status=no,menubar=no,modal=yes,scrollbars=yes');";

            var bill1NewVal = document.getElementById("<%= hdnNewBill1.ClientID %>").value;
            if (bill1NewVal) {
                document.getElementById('browseBill1').style.display = 'none';
                document.getElementById('viewBill1').style.display = '';
                document.getElementById('viewlink1').setAttribute("onclick", ViewHTML.replace("_REPLACEURL", '../Uploads/UtilityBills/' + bill1NewVal + '.jpg'));
            } else {
                document.getElementById('browseBill1').style.display = '';
                document.getElementById('viewBill1').style.display = 'none';
            }

            var bill1NewVal1 = document.getElementById("<%= hdnNewBill2.ClientID %>").value;
            if (bill1NewVal1) {
                document.getElementById('browseBill2').style.display = 'none';
                document.getElementById('viewBill2').style.display = '';
                //document.getElementById('viewlink2').href = '../Uploads/UtilityBills/' + bill1NewVal1 + '.jpg';
                document.getElementById('viewlink2').setAttribute("onclick", ViewHTML.replace("_REPLACEURL", '../Uploads/UtilityBills/' + bill1NewVal1 + '.jpg'));
            } else {
                document.getElementById('browseBill2').style.display = '';
                document.getElementById('viewBill2').style.display = 'none';
            }


            // certificates
            var certNewVal1 = document.getElementById("<%= hdnOldCerti1.ClientID %>").value;
            if (certNewVal1) {
                document.getElementById('browseCert1').style.display = 'none';
                document.getElementById('viewCert1').style.display = '';
                //  document.getElementById('viewLinkCerti1').href = '../Uploads/UtilityBills/' + certNewVal1 + '.jpg';
                document.getElementById('viewLinkCerti1').setAttribute("onclick", ViewHTML.replace("_REPLACEURL", '../Uploads/UtilityBills/' + certNewVal1 + '.jpg'));
            } else {
                document.getElementById('browseCert1').style.display = '';
                document.getElementById('viewCert1').style.display = 'none';
            }

            // certificates
            var certNewVal2 = document.getElementById("<%= hdnOldCerti2.ClientID %>").value;
            if (certNewVal2) {
                document.getElementById('browseCert2').style.display = 'none';
                document.getElementById('viewCert2').style.display = '';
                //document.getElementById('viewLinkCerti2').href = '../Uploads/UtilityBills/' + certNewVal2 + '.jpg';
                document.getElementById('viewLinkCerti2').setAttribute("onclick", ViewHTML.replace("_REPLACEURL", '../Uploads/UtilityBills/' + certNewVal2 + '.jpg'));
            } else {
                document.getElementById('browseCert2').style.display = '';
                document.getElementById('viewCert2').style.display = 'none';
            }


        }


        function ShowHideRelation(val, savedVal) {

            if (!savedVal || savedVal == '')
                savedVal = 1;
            if (val == '0') {

                document.getElementById("<%= txtDivorcedPeriod.ClientID %>").value = savedVal;
                document.getElementById('relation_row1').style.display = "none";
                document.getElementById('relation_row2').style.display = "none";
                document.getElementById('relation_row3').style.display = "none";

//            ValidatorEnable(validatorId, false);
            } else {
                document.getElementById("<%= txtDivorcedPeriod.ClientID %>").value = savedVal;
                document.getElementById('relation_row1').style.display = "";
                document.getElementById('relation_row2').style.display = "";
                document.getElementById('relation_row3').style.display = "";
//            ValidatorEnable(validatorId, true);
            }
        }

        function ShowHideMOtherAlive(val, savedVal) {

            if (!savedVal || savedVal == '')
                savedVal = 1;
            if (val == '1') {

                document.getElementById("<%= txtMotherAge.ClientID %>").value = savedVal;
                document.getElementById('ismotheralive_row').style.display = "none";
            } else {
                document.getElementById("<%= txtMotherAge.ClientID %>").value = savedVal;
                document.getElementById('ismotheralive_row').style.display = "";
            }
        }

        function ShowHideFatherAlive(val, savedVal) {

            if (!savedVal || savedVal == '')
                savedVal = 1;
            if (val == '1') {

                document.getElementById("<%= txtFatherAge.ClientID %>").value = savedVal;
                document.getElementById('isfatheralive_row').style.display = "none";
            } else {
                document.getElementById("<%= txtFatherAge.ClientID %>").value = savedVal;
                document.getElementById('isfatheralive_row').style.display = "";
            }
        }

    </script>
